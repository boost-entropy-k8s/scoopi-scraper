<?xml version="1.0"?>

<!DOCTYPE suppressions PUBLIC
"-//Puppy Crawl//DTD Suppressions 1.1//EN"
"http://checkstyle.sourceforge.net/dtds/suppressions_1_1.dtd">

<suppressions>
    <!-- disable all check for jaxb xjc generated model files  -->
    <suppress files="Adapter1.java" checks="[a-zA-Z0-9]*" />
    <suppress files="Bean.java" checks="[a-zA-Z0-9]*" />
    <suppress files="DataDef.java" checks="[a-zA-Z0-9]*" />
    <suppress files="DAxis.java" checks="[a-zA-Z0-9]*" />
    <suppress files="DFilter.java" checks="[a-zA-Z0-9]*" />
    <suppress files="DMember.java" checks="[a-zA-Z0-9]*" />
    <suppress files="Document.java" checks="[a-zA-Z0-9]*" />
    <suppress files="Fields.java" checks="[a-zA-Z0-9]*" />
    <suppress files="Locator.java" checks="[a-zA-Z0-9]*" />
    <suppress files="Locators.java" checks="[a-zA-Z0-9]*" />
    <suppress files="ObjectFactory.java" checks="[a-zA-Z0-9]*" />

    <!-- disable modules for specific files  -->
    <suppress files="messages.properties" checks="RegexpSingleline" />
    <suppress files=".*Test.java" checks="MagicNumber" />
    <suppress files=".*TestIT.java" checks="MagicNumber" />
    <suppress files=".*IT.java" checks="MagicNumber" />
    <suppress files=".*Test.java" checks="JavadocMethod" />
    <suppress files=".*TestIT.java" checks="JavadocMethod" />
    <suppress files=".*Test.java" checks="JavadocVariable" />
    <suppress files=".*TestIT.java" checks="JavadocVariable" />
    <suppress files=".*Test.java" checks="JavadocPackage" />
    <suppress files=".*TestIT.java" checks="JavadocPackage" />


</suppressions>
